            "folders": {
              "sites/default/files" : "777",
              "sites/default/privates" : "777"
            },
            "settings": {
              "general.settings.php" : "sites/default/settings.php"
            },
            "variables" : {
              "dbhost": "liansuola_mysql",
              "dbuser": "root",
              "dbpass": "thomas123"
            }
          },
          "erp": {
            "folders": {
              "sites/default/files" : "777",
              "sites/default/privates" : "777"
            },
            "settings": {
              "general.settings.php" : "sites/default/settings.php"
            },
            "variables" : {
              "dbhost": "liansuola_mysql",
