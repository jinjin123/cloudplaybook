    "projectname": "liansuola",
    "externalmode": "bootproxy",
    "domainname": "liansuola.com",
    "domainprefix": "dev-",
    "docker": {
      "privaterepo": "dockerpriv.kybot.io:5002",
      "username": "keithyau",
      "password": "thomas123"
    },
    "deploycode": {
