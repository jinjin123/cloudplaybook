{
   "name":"chefclient-docker-rancherserver",
   "description":"",
   "json_class":"Chef::Role",
   "default_attributes":{
      "deployuser":"root",
      "externalmode":"bootproxy",
      "projectname":"rancher",
      "docker": {
        "privaterepo":"dockerpriv.kybot.io:5002",
        "username":"keithyau",
        "password":"thomas123"
      },
      "domainname":"liansuola.com",
      "deploycode":{
         "basedirectory":"/home/keithyau/tools/rancher/",
         "localfolder":{
            "server":"nodownload",
            "client":"nodownload"
         },
         "runtime":{
            "server":{
              "tag":"latest",
              "image":"rancher/server",
              "mountlocal":"localdir",
              "mountdocker":"/var/lib/mysql",
              "ports":[
                "8080:8080"
              ],
              "customdomainprefix": "rancher-"
            },
            "client":{
              "tag": "latest",
              "image":"rancher/agent",
              "mountlocal":"multipledir",
              "mountdocker":[
                "/proc:/host/proc",
                "/dev:/host/dev",
                "/var/run/docker.sock:/var/run/docker.sock",
                "/var/run/rancher/storage:/var/run/rancher/storage",
                "/lib/modules:/lib/modules"
              ],
            },
            "bootproxy":{
               "tag":"latest",
               "image":"nginx",
               "mountlocal":"localdir",
               "mountdocker":"/etc/nginx/conf.d/",
               "ports":[
                  "80:80"
               ]
            }
         }
      }
   },
   "override_attributes":{

   },
   "chef_type":"role",
   "run_list":[
      "recipe[git]",
      "recipe[build-essential]",
      "recipe[basicservice]",
      "recipe[webserver]",
      "recipe[deploycode]"
   ],
   "env_run_lists":{

   }
}
