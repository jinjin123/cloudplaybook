{
   "name":"chefsolo",
   "description":"",
   "json_class":"Chef::Role",
   "default_attributes":{
      "deployuser":"root",
      "externalmode":"bootproxy",
      "projectname":"shadowdock",
      "domainname":"shadowdock.com",
      "domainprefix":"dev-",
      "docker": {
          "privaterepo":"dockerpriv.shadowdock.com",
          "username":"keithyau",
          "password":"thomas123"
      },
      "deploycode":{
         "basedirectory":"/home/keithyau/bootdev/shadowdock/",
         "default":{
            "variables":{
               "dbuser":"root",
               "dbpass":"thomas123",
               "dbhost":"shadowdock_mysql"
            }
         },
         "configuration":{
         },
         "localfolder":{
            "mysql":"nodownload",
            "demoshadowsocks":"nodownload",
            "bootproxy":"nodownload"
         },
         "runtime":{
            "mysql":{
               "tag":"latest",
               "image":"mysql",
               "env":["MYSQL_ROOT_PASSWORD=thomas123"],
               "mountlocal":"/root/mysqldir",
               "mountdocker":"/var/lib/mysql",
               "ports":[
                  "3306:3306"
               ]
            },
            "demoshadowsocks":{
               "tag":"latest",
               "image":"mritd/shadowsocks",
               "command": "-k Thomas123 -w 2 -f",
               "customdomainprefix": "user1customhash",
               "mountlocal":"localdir",
               "mountdocker":"/mnt",
               "proxyport":"5000",
               "ports":[
                  "5000:5000",
                  "20000:20000/udp"
               ]
            },
            "bootproxy":{
               "tag":"stable-alpine",
               "image":"nginx",
               "mountlocal":"localdir",
               "mountdocker":"/etc/nginx/conf.d/",
               "ports":[
                  "80:80"
               ]
            }
         }
      }
   },
   "override_attributes":{

   },
   "chef_type":"role",
   "run_list":[
      "recipe[git]",
      "recipe[build-essential]",
      "recipe[basicservice]",
      "recipe[glusterfs]",
      "recipe[webserver]",
      "recipe[deploycode]",
      "recipe[drupalsetting]"
   ],
   "env_run_lists":{

   }
}
