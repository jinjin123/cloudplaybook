{
   "name":"chefclient-sparkpad-sparkposcn",
   "description":"",
   "json_class":"Chef::Role",
   "default_attributes":{
      "deployuser":"root",
      "projectname":"sparkpad",
      "externalmode":"bootproxy",
      "domainname":"sparkpos.cn",
      "deploycode":{
        "basedirectory":"/root/tools/code/",
        "localfolder":{
            "sparkposcn":{
              "giturl": "git@git.coding.net:chankongching/sparkpos.cn.git",
              "branch": "master"
            },
            "bootproxy":"nodownload"
         },
         "runtime":{
            "sparkposcn":{
                "tag":"stable-alpine",
                "image":"daocloud.io/library/nginx",
                "mountlocal":"localdir",
                "mountdocker":"/usr/share/nginx/html",
                "overridesubdomain":"www",
                "ports":"8081:80"
            },
            "bootproxy":{
                "tag":"stable-alpine",
                "image":"daocloud.io/library/nginx",
                "mountlocal":"multipledir",
                "mountdocker": [
                  "/root/tools/code/bootproxy:/etc/nginx/conf.d/",
                  "/usr/bin/curl:/usr/bin/curl"
                ],
                "ports":[
                  "80:80"
                ]
            }
         }
      }
   },
   "override_attributes":{

   },
   "chef_type":"role",
   "run_list":[
      "recipe[git]",
      "recipe[build-essential]",
      "recipe[basicservice]",
      "recipe[deploycode]",
      "recipe[webserver]"
   ],
   "env_run_lists":{

   }
}
