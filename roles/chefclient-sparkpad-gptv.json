{
   "name":"chefclient-sparkpad-gptv",
   "description":"",
   "json_class":"Chef::Role",
   "default_attributes":{
      "deployuser":"root",
      "projectname":"sparkpad",
      "externalmode":"bootproxy",
      "domainname":"liansuola.com",
      "docker": {
          "privaterepo":"dockerpriv.kybot.io:5002",
          "username":"keithyau",
          "password":"thomas123"
      },
      "deploycode":{
        "basedirectory":"/home/sparkpad/code/sparkpad/",
        "configuration": {
          "general" : {
            "de": {
              "spzkf.nginx.conf": "/usr/local/nginx/conf/nginx.conf"
            }
          },
          "drupal" : {
            "sparkpad_tvbackend_drupal": {
              "folders": {
                "sites/default/files" : "777",
                "sites/default/privates" : "777"
              },
              "settings": {
                "general.settings.php" : "sites/default/settings.php"
              },
              "variables" : {
                "dbhost": "sparkpad_mysql",
                "dbuser": "root",
                "dbpass": "thomas123"
              }
            }
          }
        },
        "localfolder":{
            "mysql":"nodownload",
            "sparkpad_tvbackend_drupal":{
              "giturl": "git@git.coding.net:chankongching/sparkpad_tvbackend_drupal.git",
              "branch": "master"
            },
            "bootproxy":"nodownload"
         },
         "runtime":{
            "mysql":{
              "tag":"7.0.17_v4",
              "image":"ddockerpriv.kybot.io:5002/keithyau/drupalnginxphp7shanghai",
              "env":["MYSQL_ROOT_PASSWORD=thomas123"],
              "mountlocal":"/home/sparkpad/sparkpadmysql",
              "mountdocker":"/var/lib/mysql"
            },
            "sparkpad_tvbackend_drupal":{
              "tag":"7.0.17_v4",
              "image":"dockerpriv.kybot.io:5002/keithyau/drupalnginxphp7shanghai",
              "env":["NEWRELICKEY=44d69e5dd03a38d50a94161137ad0e44f33761da","NEWRELICAPPNAME=liansuolaTV"],
              "mountlocal":"localdir",
              "mountdocker": "/var/www/html",
              "ports":"7052:80",
              "exec": "ln -sf /dev/stderr /var/log/php-fpm.log",
              "overridesubdomain":"tv"
            },
            "bootproxy":{
               "tag":"7.0.17_v4",
               "image":"dockerpriv.kybot.io:5002/keithyau/drupalnginxphp7shanghai",
               "mountlocal":"localdir",
               "mountdocker":"/etc/nginx/conf.d/",
               "ports":[
                  "80:80"
               ]
            }
         }
      }
   },
   "override_attributes":{

   },
   "chef_type":"role",
   "run_list":[
      "recipe[git]",
      "recipe[build-essential]",
      "recipe[basicservice]",
      "recipe[deploycode]",
      "recipe[drupalsetting]",
      "recipe[webserver]"
   ],
   "env_run_lists":{

   }
}
